<?xml version="1.0" encoding="UTF-8"?>
<project default="createJar" name="<<Enter Jar Name Here>>">
	<property name="bin" value="${basedir}/bin" />
	<property name="jarName" value="exility.turtle.jar"/>
	<property name="jar" value="${basedir}/${jarName}" />
	<target name="createJar" description="Create jar file">
		<jar basedir="${bin}" jarfile="${jar}" />
		<copy file="${jar}" overwrite="true" todir="<<Enter Output Directory Here>>"></copy>
	</target>
</project>